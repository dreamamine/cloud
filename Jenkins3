pipeline {
    agent any
    stage ('Building docker image') {
            steps {
               
                    sh 'docker build -t dock .'
                  }
        }

	stage ('Again rest') {
            steps {

                    sh 'sleep 10'

            }
        }
	stage ('Pushing docker image to registry') {
            steps {

                    sh 'docker push imech/amine'

            }
        }


    }

}
